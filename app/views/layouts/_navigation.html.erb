<%= link_to "Chatroom", root_path, class: "item" %>
<a class="item">
  Messages
</a>
<div class="right menu">
  <% if !logged_in? %>
    <div class="item">
      <%= link_to "Log in", login_path, class: "ui orange button" %>
    </div>
  <% else %>
    <div class="item">
      <%= link_to 'Log out', logout_path, class:"ui orange button", data: {turbo_method: :delete}%>
    </div>
  <% end %>
</div>
